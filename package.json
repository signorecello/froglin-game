    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@types/node": "^20.12.2",
    "@zk-kit/eddsa-poseidon": "^0.6.0",
    "@zk-kit/smt": "https://gitpkg.now.sh/signorecello/zk-kit/packages/smt?zpedro/smt_pedersen"
